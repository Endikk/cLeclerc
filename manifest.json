{
  "name": "cLeclerc",
  "version": "1.0",
  "description": "Remplace toutes les images sur les sites web par des photos de Charles Leclerc",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-1.8.0.min.js", "cleclerc.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["cleclerc.js"],
      "matches": ["<all_urls>"]
    }
  ]
}